        "Fabric:Policies:Global:DnsProfiles": {
            "type": "object",
            "properties": {
                "annotation": {
                    "type": "string",
                    "description": "An annotation will mark an Object in the GUI with a small blue circle, signifying that it has been modified by  an external source/tool.  Like Nexus Dashboard Orchestrator or in this instance Terraform.",
                    "minLength": 1,
                    "maxLength": 128,
                    "pattern": "^[a-zA-Z0-9\\\\!#$%()*,-\\./:;@ _{|}~?&+]+$"
                },
                "description": {
                    "type": "string",
                    "description": "Description to add to the Object.  The description can be up to 128 characters.",
                    "minLength": 1,
                    "maxLength": 128,
                    "pattern": "^[a-zA-Z0-9\\\\!#$%()*,-\\./:;@ _{|}~?&+]+$"
                },
                "dns_domains": {
                    "type": "array",
                    "description": "A List of Layer3 Domains to Attach to this AAEP Policy.",
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "description": "Name of a L3 Domain Policy",
                        "properties": {
                            "domain": {
                                "type": "string",
                                "description": "Description to add to the Object.  The description can be up to 128 characters.",
                                "minLength": 1,
                                "maxLength": 128,
                                "pattern": "^(?!-)[A-Za-z0-9-]+([\\-\\.]{1}[a-z0-9]+)*\\.[A-Za-z]{2,6}$"
                            },
                            "default_domain": {
                                "type": "boolean",
                                "default": false,
                                "description": "Description to add to the Object.  The description can be up to 128 characters."
                            },
                            "description": {
                                "type": "string",
                                "description": "Description to add to the Object.  The description can be up to 128 characters.",
                                "minLength": 1,
                                "maxLength": 128,
                                "pattern": "^[a-zA-Z0-9\\\\!#$%()*,-\\./:;@ _{|}~?&+]+$"
                            }
                                    
                        },
                        "required": [
                            "domain"
                        ]
                    }
                },
                "dns_providers": {
                    "type": "array",
                    "description": "A List of Layer3 Domains to Attach to this AAEP Policy.",
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "description": "Name of a L3 Domain Policy",
                        "properties": {
                            "description": {
                                "type": "string",
                                "description": "Description to add to the Object.  The description can be up to 128 characters.",
                                "minLength": 1,
                                "maxLength": 128,
                                "pattern": "^[a-zA-Z0-9\\\\!#$%()*,-\\./:;@ _{|}~?&+]+$"
                            },
                            "dns_provider": {
                                "type": "string",
                                "description": "Description to add to the Object.  The description can be up to 128 characters.",
                                "minLength": 7,
                                "maxLength": 128,
                                "pattern": "^\\s*((([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5]))\\s*$)|(^\\s*((([0-9A-Fa-f]{1,4}:){7}([0-9A-Fa-f]{1,4}|:))|(([0-9A-Fa-f]{1,4}:){6}(:[0-9A-Fa-f]{1,4}|((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(\\.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){5}(((:[0-9A-Fa-f]{1,4}){1,2})|:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(\\.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3})|:))|(([0-9A-Fa-f]{1,4}:){4}(((:[0-9A-Fa-f]{1,4}){1,3})|((:[0-9A-Fa-f]{1,4})?:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(\\.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){3}(((:[0-9A-Fa-f]{1,4}){1,4})|((:[0-9A-Fa-f]{1,4}){0,2}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(\\.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){2}(((:[0-9A-Fa-f]{1,4}){1,5})|((:[0-9A-Fa-f]{1,4}){0,3}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(\\.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(([0-9A-Fa-f]{1,4}:){1}(((:[0-9A-Fa-f]{1,4}){1,6})|((:[0-9A-Fa-f]{1,4}){0,4}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(\\.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:))|(:(((:[0-9A-Fa-f]{1,4}){1,7})|((:[0-9A-Fa-f]{1,4}){0,5}:((25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)(\\.(25[0-5]|2[0-4]\\d|1\\d\\d|[1-9]?\\d)){3}))|:)))(%.+)?\\s*$"
                            },
                            "preferred": {
                                "type": "boolean",
                                "default": false,
                                "description": "Description to add to the Object.  The description can be up to 128 characters."
                            }
                        },
                        "required": [
                            "dns_provider"
                        ]
                    }
                },
                "ip_version_preference": {
                    "type": "string",
                    "default": "IPv4",
                    "description": "Name of the Attachable Access Entity Profile Policy.",
                    "enum": [
                        "IPv4",
                        "IPv6"
                    ]
                },
                "management_epg": {
                    "type": "string",
                    "default": "default",
                    "description": "Name of the Attachable Access Entity Profile Policy.",
                    "minLength": 1,
                    "maxLength": 64,
                    "pattern": "^[a-zA-Z0-9_\\.:-]+$"
                },
                "name": {
                    "type": "string",
                    "default": "default",
                    "description": "Name of the Attachable Access Entity Profile Policy.",
                    "minLength": 1,
                    "maxLength": 64,
                    "pattern": "^[a-zA-Z0-9_\\.:-]+$"
                }
            },
            "required": [
                "dns_domains",
                "dns_providers",
                "name"
            ],
            "title": "dns_profiles - DNS Profiles"
        },
