        },
        "Fabric:Policies:Pod:DateAndTime": {
            "type": "object",
            "properties": {
                "administrative_state": {
                    "type": "string",
                    "default": "enabled",
                    "description": "The NTP protocol can be enabled or disabled. If enabled, the system uses the NTP protocol. The NTP protocol can be either of the following:\n  * enabled: (default) — NTP protocol is enabled.\n  * disabled — NTP protocol is disabled.",
                    "enum": [
                        "disabled",
                        "enabled"
                    ],
                    "title": "administrative_state"
                },
                "annotation": {
                    "type": "string",
                    "description": "An annotation will mark an Object in the GUI with a small blue circle, signifying that it has been modified by  an external source/tool.  Like Nexus Dashboard Orchestrator or in this instance Terraform.",
                    "minLength": 1,
                    "maxLength": 128,
                    "pattern": "^[a-zA-Z0-9\\\\!#$%()*,-\\./:;@ _{|}~?&+]+$",
                    "title": "annotation"
                },
                "authentication_keys": {
                    "type": "array",
                    "additionalProperties": false,
                    "description": "List of NTP Authentication keys; when performing NTP Authentication.  Required Attributes are:\nkey_id\n\nOptional Attributes are:\n  * authentication_type: (default)\n  * external_epg",
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "description": "authentication_keys - Required Attributes are:\n  * key_id\n\nOptional Attributes are:\n  * authentication_type\n  * trusted",
                        "properties": {
                            "authentication_type": {
                                "type": "string",
                                "default": "sha1",
                                "description": "authentication_type: (optional) — The authentication key type can be:\n  * md5 — Use HMAC MD5 algorithm for authentication\n  * sha1: (default) — Use HMAC SHA1 algorithm for authentication",
                                "enum": [
                                    "md5",
                                    "sha1"
                                ],
                                "title": "authentication_type"
                            },
                            "key_id": {
                                "type": "integer",
                                "default": 1,
                                "description": "The user-configured NTP client authentication key identifier. The NTP client authentication key is for authenticating time synchronization.  This is a reference to the Sensitive Variable ntp_key_{1-5}, which should be loaded into the environment as TF_VAR_ntp_key_{1-5}",
                                "minimum": 1,
                                "maximum": 5,
                                "title": "key_id"
                            },
                            "trusted": {
                                "type": "boolean",
                                "default": false,
                                "description": "The trust status of the authentication key. This indicates if the NTP authentication key is trusted. The trust states of the NTP authentication key follows:\n  * false — The authentication key is not trusted.\n  * true: (default) — The authentication key is trusted.",
                                "title": "trusted"
                            }
                        }
                    },
                    "required": [
                        "key_id"
                    ],
                    "title": "authentication_keys"
                },
                "description": {
                    "type": "string",
                    "description": "Description to add to the Object.  The description can be up to 128 characters.",
                    "minLength": 1,
                    "maxLength": 128,
                    "pattern": "^[a-zA-Z0-9\\\\!#$%()*,-\\./:;@ _{|}~?&+]+$",
                    "title": "description"
                },
                "display_format": {
                    "type": "string",
                    "default": "local",
                    "description": "The date/time display format selector. The display format can be UTC or local. The date/time display format can be either of the following:\n  * local: (default) — Date/time is displayed in local time.\n  * utc — Date/time is displayed in UTC.",
                    "enum": [
                        "local",
                        "utc"
                    ],
                    "title": "display_format"
                },
                "management_epg": {
                    "type": "string",
                    "default": "default",
